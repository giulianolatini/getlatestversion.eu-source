{
    "workbench.iconTheme": "vscode-icons",
    "files.autoSave": "afterDelay",
    "workbench.sideBar.location": "right",
    "editor.fontFamily": "'Cascadia Code', Consolas, 'Courier New', monospace",
    "editor.fontWeight": "300",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "debug.console.fontSize": 16,
    "markdown.preview.fontSize": 16,
    "terminal.integrated.fontFamily": "Cascadia Code PL",
    "terminal.integrated.fontWeight": "300",
    "terminal.integrated.fontSize": 16,
    "yaml.schemas": {
        "file:///toc.schema.json": "/toc\\.yml/i"
    },
    "git.autofetch": true,
    "git.enableCommitSigning": true,
    "markdownlint.customRules": [
        "{docsmsft.docs-linting}/markdownlint-custom-rules/rules.js"
    ],
    "markdownlint.config": {
        "MD025": {
            "front_matter_title": ""
        },
        "MD028": false
    },
    "markdown.defaultView": "source",
    "hediet.vscode-drawio.local-storage": "",
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\wsl.exe",
    "markdown.preview.fontFamily": "'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif, -apple-system, BlinkMacSystemFont,",
    "preview.previewTheme": "Dark",
    "workbench.list.openMode": "doubleClick",
    "workbench.editor.showTabs": true,
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "markdown-preview-enhanced.codeBlockTheme": "github.css",
    "markdown-preview-enhanced.HTML5EmbedUseLinkSyntax": true,
    "markdown-preview-enhanced.mathRenderingOnlineService": "https://latex.codecogs.com/png.latex",
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "search.useReplacePreview": true,
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "markdown.preview.openMarkdownLinks": "inEditor",
    "editor.rename.enablePreview": false,
    "window.zoomLevel": 0,
    "markdown-preview-enhanced.mathBlockDelimiters": [
    
        [
            "$$",
            "$$"
        ],
        [
            "\\[",
            "\\]"
        ]
    ],
    "editor.minimap.enabled": false,
    "markdown.extension.preview.autoShowPreviewToSide": false,
    "editor.quickSuggestions": true,
    "diffEditor.renderSideBySide": true,
    "docker.dockerComposeBuild": false
}
    